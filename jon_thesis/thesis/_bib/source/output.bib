@article{ADM:Bys2000,
  Author = {Bystroff, C. and Thorsson, V. and Baker, D.},
  Title = {HMMSTR: a hidden Markov model for local sequence-structure correlations in proteins},
  Journal = {J Mol Biol},
  Volume = {301},
